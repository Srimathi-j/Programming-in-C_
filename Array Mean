#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int main() {
    int n, val, sum = 0;
    float mean;

    scanf("%d", &n);

    for (int i = 0; i < n; i++) {
        scanf("%d", &val);
        sum += val;
    }

    mean = (float)sum / n;
    printf("Array Mean Value is %.2f\n", mean);

    return 0;
}
