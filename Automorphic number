#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
     int a;
    scanf("%d",&a);
    int b=a*a;
    int l=b%10;
     if(a<=9 && a>0 && a<10 && l==a){
         printf("Automorphic Number");
     }
    else if(a<=9 && a>0&& a<10 &&l!=a){
     printf("Not Automorphic Number");
             
         }
    else
    {
        printf("Invalid Input");
    }
    return 0;
}
