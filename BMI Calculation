#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    float a,b;
    scanf("%f%f",&a,&b);
    int c=(a/(b*b))*10000;
    if(c<18.5){
        printf("You are underweight. Have an apple daily.");
    }
    else if(c>=18.5 && c<=24.9){
            printf("You are normal. Go walking daily and maintain it.");
    }      
     else if(c>24.9 && c<=29.9){
    printf("You are overweight. Go to Gym daily.");
     }
    else{
        printf("You are obese. You need to see a doctor.");
    }
    return 0;
}
