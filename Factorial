#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>



int main() {
    int num;
    long long factorial = 1;
    scanf("%d", &num);

    if (num < 0) {
        printf("Invalid Input\n");
    } else {
        for (int i = 1; i <= num; i++) {
            factorial *= i;
        }
        printf("The Factorial of %d is %lld\n", num, factorial);
    }

    return 0;
}
