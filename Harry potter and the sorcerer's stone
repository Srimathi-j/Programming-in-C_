#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int a;
    scanf("%d",&a);
    int l=a%10;
    int f=a/1000;
    printf("%d",f+l);
    
    return 0;
}
