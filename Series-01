#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>



int main() {
    int n, term;
    scanf("%d", &n);

    term = 6;
    for (int i = 1; i <= n; i++) {
        printf("%d", term);
        if (i != n) {
            printf(" ");
        }
        term += (2 * i + 1);
    }

    return 0;
}
