#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
int a;
    float b;
    char c;
    char d[40];
    char e[50];
    scanf("%d",&a);
    scanf("%f ",&b);
    scanf("%c ",&c);
    scanf("%s ",d);
    fgets(e,50,stdin);
    printf("Integer is %d\n",a);
    printf("Float is %.2f \n",b);
    printf("Character is %c\n",c);
    printf("Strings are %s and %s",d,e);
    return 0;
}
