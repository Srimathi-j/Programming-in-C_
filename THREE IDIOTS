#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */  
    int x1,y1,x2,y2;
    scanf("%d%d%d%d",&x1,&y1,&x2,&y2);
    float a=(x1+x2)/2.0;
    float b=(y1+y2)/2.0;
    printf("Binoy's house is located at (%.1f,%.1f)",(float)a,(float)b);
    return 0;
}
