#include <math.h>
#include <stdio.h>
#include <string.h>
#include <stdlib.h>
#include <assert.h>
#include <limits.h>
#include <stdbool.h>

int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */
    int w,x,y;
    scanf("%d%d%d",&w,&x,&y);
    int p=(w*x)-(w*y)-100;
    printf("%d",p);
    
    
    
    
    return 0;
}
