#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int a;
    char c;
    scanf("%d ",&a);
    scanf("%c",&c);
    
    
    printf("Integer of %d is %c\n",a,(char)a);
    printf("Character of %c is %d",c,(int)c);
    return 0;
}
